<h1>Listing product_person_maps</h1>

<table>
  <thead>
    <tr>
      <th>Product</th>
      <th>Person</th>
      <th>Rank</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_person_maps.each do |product_person_map| %>
      <tr>
        <td><%= product_person_map.product %></td>
        <td><%= product_person_map.person %></td>
        <td><%= product_person_map.rank %></td>
        <td><%= link_to 'Show', product_person_map %></td>
        <td><%= link_to 'Edit', edit_product_person_map_path(product_person_map) %></td>
        <td><%= link_to 'Destroy', product_person_map, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product person map', new_product_person_map_path %>
