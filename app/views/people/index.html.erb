<h1>Listing people</h1>

<table>
  <thead>
    <tr>
      <th>Lastname</th>
      <th>Firstname</th>
      <th>Middleinitial</th>
      <th>Webname</th>
      <th>Email</th>
      <th>Url</th>
      <th>Urlphoto</th>
      <th>Phonework</th>
      <th>Phonehome</th>
      <th>Phonecell</th>
      <th>Fax</th>
      <th>Addressoffice</th>
      <th>Addresslab</th>
      <th>Addressmail</th>
      <th>Department</th>
      <th>Urldepartment</th>
      <th>Organization</th>
      <th>Urlorganization</th>
      <th>Biography</th>
      <th>Researchinterests</th>
      <th>Education</th>
      <th>Professionalexperience</th>
      <th>Professionalservice</th>
      <th>Otherpublications</th>
      <th>Patents</th>
